.infor 
{
    height: 85px;
    display: table;
}
div.toolbar 
{
    background: repeat-x scroll left top #EAEAEA;
    border-bottom: 1px solid #DDDDDD;
}
div.toolbar ul 
{
    float: left;
}
div.toolbar ul li 
{
    border-right: 1px solid #D0D0D0;
    display: block;
    float: left;
}
div.toolbar ul li a, div.toolbar ul li span 
{
    box-shadow: 1px 0 0 rgba(255, 255, 255, 0.75) inset, 0 1px 0 #FFFFFF inset;
    color: #666666;
    cursor: pointer;
    display: block;
    padding: 8px 12px;
    text-decoration: none;
}

div.addmenu img 
{
    vertical-align: middle;
}
div.addmenu a 
{
    border: 1px solid #BBBBBB;
    border-radius: 4px;
    box-shadow: 0 1px #FFFFFF inset, 0 0 0 1px rgba(255, 255, 255, 0.7) inset, 0 2px #DADADA, 0 2px #C4C4C4, 0 2px 3px rgba(0, 0, 0, 0.1);
    padding: 10px;
}
div.addmenuFooter 
{
    margin: 15px 5px 5px;
    text-align: right;
}
div.addmenuFooter img 
{
    vertical-align: middle;
}
div.addmenuFooter a 
{
    border: 1px solid #BBBBBB;
    border-radius: 4px;
    box-shadow: 0 1px #FFFFFF inset, 0 0 0 1px rgba(255, 255, 255, 0.7) inset, 0 2px #DADADA, 0 2px #C4C4C4, 0 2px 3px rgba(0, 0, 0, 0.1);
    padding: 10px;
}

