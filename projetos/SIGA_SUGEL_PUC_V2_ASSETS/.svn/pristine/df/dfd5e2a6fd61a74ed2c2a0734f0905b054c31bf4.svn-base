.resultado 
{
    -moz-box-sizing: border-box;
    background: #EDEFF0;
    border: 1px solid #CCCCCC;
    border-radius: 6px;
    color: #323232;
    cursor: pointer;
    float: left;
    height: 35px;
    margin: 5px 0 0 5px;
    padding-right: 10px;
    text-decoration: none;
    position: relative;
}

.resultado2
{
    -moz-box-sizing: border-box;
    background: #EDEFF0;
    border-radius: 6px;
    color: #323232;
    cursor: pointer;
    float: left;
    height: 35px;
    margin: 5px 8px 8px 10px;
    padding-right: 10px;
    text-decoration: none;
    position: relative;
}
