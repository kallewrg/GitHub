body { 
  background: url(../images/bodyBg.png);
}

p {
  margin: 0 0 5px;
}

/*TEM QUE VER*/
small,
.small {
  font-size: none;
}

/*TEM QUE VER*/
.row {
margin-right: 0px;
margin-left: 0px;
}

.table {
  margin-bottom: 0px; #20px;
  margin-top: 0px; #20px;
}

.table > thead > tr > th,
.table > tbody > tr > th,
.table > tfoot > tr > th,
.table > thead > tr > td,
.table > tbody > tr > td,
.table > tfoot > tr > td {
  padding: 4px;
  font-size: 12px;
}
.form-control {
  height: 30px;
}
.form-group {
  margin-bottom: 8px;
}
.form-horizontal {
  margin-top:20px;
}
.nav > li > a {
  position: relative;
  display: block;
  padding: 5px 10px;
  -moz-border-bottom-colors: none;
  -moz-border-left-colors: none;
  -moz-border-right-colors: none;
  -moz-border-top-colors: none;
  background-color: #f6f6f6;
  border-color: #CFCFCF #D6D6D6 #C8C8C8;
  border-image: none;
  border-style: solid;
  border-width: 1px;
  box-shadow: 0 1px #FFFFFF inset, 0 0 0 1px rgba(255, 255, 255, 0.7) inset, 0 2px #DADADA, 0 3px #C4C4C4, 0 4px 3px rgba(0, 0, 0, 0.1);
}
.nav > li.disabled > a:hover,
.nav > li.disabled > a:focus {
  background-color: #eeeeee;
}

/*TEM QUE VER*/
.nav .open > a,
.nav .open > a:hover,
.nav .open > a:focus {
  background-color: #eeeeee;
  border-color: transparent;
}
/*TEM QUE VER*/
.nav-tabs {
  border-bottom: 1px transparent;
  position: relative;
  top: -32px;
}
.nav-tabs > li > a {
  margin-right: 2px;
  line-height: 1.428571429;
  border: 1px solid transparent;
  border-radius: 4px 4px 0 0;
  background-color: #ddd;
}

/*TEM QUE VER*/
.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover,
.nav-tabs > li.active > a:focus {
  color: #555555;
  cursor: default;
  background-color: none;
  border: 1px solid transparent;
  border-bottom-color: transparent;
  font-weight:bold;
  background: none repeat scroll 0 0 #F2F3F4;
}

.navbar-collapse.in {
  overflow-y: none;
}

.navbar-toggle {
  padding: 7px 8px;
  margin-top: 15px;
  margin-right: 45px;
}
.navbar-toggle .icon-bar {
  width: 16px;
}
@media (max-width: 767px) {
  .navbar-nav .open .dropdown-menu {
    background-color: #eee;
  }
}

@media (min-width: 768px) {
  .navbar-nav > li > a {
    padding-top: none;
    padding-bottom: none;
    padding: 8px 11px;
    border-left-color: #C6C6C6;
    border-left-width: 1px !important;
    border-radius: 5px;
  }
}

.navbar-inverse {
  border: 0;
  border-color: 0px;
  background-color: inherit;
}
.navbar-inverse .navbar-nav > li > a {
  color: #595959;
}
.navbar-inverse .navbar-nav > li > a:hover,
.navbar-inverse .navbar-nav > li > a:focus {
  color: #2BA6CB;
  background-color: #eeeeee;
}
.navbar-inverse .navbar-nav > .active > a,
.navbar-inverse .navbar-nav > .active > a:hover,
.navbar-inverse .navbar-nav > .active > a:focus {
  color: #595959;
  background-color: #eeeeee
}
.navbar-inverse .navbar-nav > .active > a:hover{
  color: #2BA6CB;
}
.navbar-inverse .navbar-toggle {
  border-color: #ccc;
}
.navbar-inverse .navbar-toggle:hover,
.navbar-inverse .navbar-toggle:focus {
  background-color: #999;
}
.navbar-inverse .navbar-nav > .open > a,
.navbar-inverse .navbar-nav > .open > a:hover,
.navbar-inverse .navbar-nav > .open > a:focus {
  color: #2BA6CB;
  background-color: none;
}
@media (max-width: 767px) {
.navbar-inverse .navbar-nav .open .dropdown-menu > li > a:hover,
  .navbar-inverse .navbar-nav .open .dropdown-menu > li > a:focus {
    color: #428BCA;
    background-color: transparent;
  } 
}
.breadcrumb {
  padding: 8px 17px;
  margin-bottom: 20px;
  list-style: none;
  background: url(../images/paper.png) #ACC2CF;
  border-radius: 4px;
}
.breadcrumb > .active {
  padding: 0;
  color: none;
  content: none;
   color: #999999;
}
.breadcrumb {
   font-size: 12px;
    height: 32px;
    list-style: none outside none;
    margin-bottom: 30px;
    padding: 0 0 0 30px;
    position: fixed;
    top: 140px;
    width: 100%;
    z-index: 2;
}
.breadcrumb li {
    background: url(../images/back-breadcrumb.png) no-repeat scroll right -85px rgba(0, 0, 0, 0);
    float: left;
    height: 30px;
    padding: 0 28px 0 10px;
}
.breadcrumb li.last {
    background: url(../images/back-breadcrumb.png) no-repeat scroll right -45px rgba(0, 0, 0, 0);
}
.breadcrumb li a {
    color: #757F8C;
    display: block;
    height: 40px;
    line-height: 30px;
    text-shadow: 0 1px #FEFEFF;
}
.breadcrumb li.home {
    height: 30px;
    padding: 0 5px;
    width: 50px;
}
.breadcrumb li.home a {
    background: url(../images/home2.png) no-repeat scroll 5px 7px rgba(0, 0, 0, 0);
}
.breadcrumb > li + li:before {
  padding: none;
  color: none;
  content: none;
}
.pagination > .itensTotal > span{
  color: #428BCA;
}
.pagination > .itensTotal > a:hover,
.pagination > .itensTotal > span:hover{
  background-color: #fff; 
  color: #428BCA;
}
.panel-default > .panel-heading {
  border-top: 2px solid #368CA9;
}

