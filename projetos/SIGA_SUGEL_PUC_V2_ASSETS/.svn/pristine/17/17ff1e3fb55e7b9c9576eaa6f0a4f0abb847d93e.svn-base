            <script type="text/javascript">
  $(document).ready(function () {
      $('#sancaoFornecedorBuscar').submit(function () {
          if (
           !$("#cnpjFornecedor").val()
          && !$("#nomeFornecedor").val()
          && !$("#efeitoRestritivo").val()
          && !$("#fundamentoLegal").val()
          && !$("#situacaoSancao").val()
          && !$("#statusSancao").val()
          && !$("#orgaoSancionador").val()
          && !$("#multa").val()
          && !$("#mandatoSeguranca").val())
          {
              var texto = "Informe um parâmetro de busca.";
              error(texto);
              return false;
          }
      });

      $('#sancaoFornecedoLimpar').click(function () {
          $('#sancaoFornecedorBuscar').trigger("reset");
      });

  });

  function error(texto) {
      $("#mensagens #texto").html(texto);
      $("#mensagens").modal('show');
  }
</script>

