            <script type="text/javascript">
			function submitForm(action) {

				var form = $('#mainForm');
				form.attr('action', action);
				form.submit();
			}

			function submitBusca() {

				var action = "/SIGATESTES/administracao/Minuta/apresentaListar.action";
				var tipoBusca = $("input:radio[name ='tipoBusca']:checked")
						.val();

				if (tipoBusca == '1') {
					action = "/SIGATESTES/administracao/ModeloPGE/apresentaListar.action";
				}

				submitForm(action);

			}

			function submitCriarModelo() {
				submitForm("/SIGATESTES/administracao/ModeloPGE/apresentaCriar.action");
			}

			function toggleCamposPesquisa() {
				$("#inputUnidade").toggle();
				$("#inputProcesso").toggle();
				$("#inputContrato").toggle();
				$("#inputObjeto").toggle();
				$("#inputDatas").toggle();
				$("#inputTipoModelo").toggle();
				$("#inputSubtipoModelo").toggle();
				$("#inputTipoMinuta").toggle();
				$("#inputSubtipoMinuta").toggle();

			}

			function toggleTipoBusca() {
				$("#modeloSubmit").toggle();
				toggleCamposPesquisa();
			}

			function populateSubTipo(idSelect, component) {

				$
						.getJSON(
								"/SIGATESTES/administracao/Minuta/getSubTiposMinuta.action",
								'tipoMinutaSelecionado=' + component.val(),
								function(json) {
									var options = "";
									options += '<option value="">Selecionar</option>';
									if (json.subtiposMinuta != null) {
										$
												.each(
														json.subtiposMinuta,
														function(index, item) {
															options += '<option value="'
																					+ item.cdMinuta
																					+ '">'
																	+ item.tipoMinuta
																	+ '</option>';
														});
									}
									$(idSelect).find("select").html(options);
								});
			}

			jQuery(document)
					.ready(
							function($) {

								$("#mainForm").validate();

								$("#dataInicial").datepicker({
									format : "dd/mm/yyyy",

								}).on(
										'changeDate',
										function(e) {
											$("#dataFinal").datepicker(
													'setStartDate', e.date);
										});

								$("#dataFinal").datepicker({
									format : "dd/mm/yyyy",

								}).on(
										'changeDate',
										function(e) {
											$("#dataInicial").datepicker(
													'setEndDate', e.date)
										});

								var tpMinuta = $("#tpMinuta");
								var tpModelo = $("#tpModelo");

								tpMinuta.val('');
								tpModelo.val('');
								$('iptModelo').prop('checked', false);
								$('iptMinuta').prop('checked', true);

								var contratoInput = $("#inputContrato").find(
										"input");
								var objetoInput = $("#inputObjeto").find(
										"input");
								var processoInput = $("#inputProcesso").find(
										"input");

								contratoInput.attr("placeholder",
										"Número do contrato");
								objetoInput.attr("placeholder",
										"Objeto do Contrato");
								processoInput.attr("placeholder",
										"Número do Processo");

								tpMinuta
										.change(function() {

											var text = $(this).find(
													"option:selected").text();

											var contratoLabel = $(
													"#inputContrato").find(
													"label");
											var contratoInput = $(
													"#inputContrato").find(
													"input");
											var objetoInput = $("#inputObjeto")
													.find("input");

											if (text == 'Ata de Registro de Preços') {
												contratoLabel
														.empty()
														.text(
																"Ata de Registro de Preços");
												contratoInput
														.attr('placeholder',
																'Número da Ata de Registro de Preços');
												objetoInput
														.attr('placeholder',
																'Objeto da Ata de Registro de Preços');
											} else if (text == 'Edital de Licitação') {
												contratoLabel.empty().text(
														"Licitação");
												contratoInput.attr(
														'placeholder',
														'Número da Licitação');
												objetoInput.attr('placeholder',
														'Objeto da Licitação');
											} else if (text == 'Termo de Contrato') {
												contratoLabel.empty().text(
														"Contrato");
												contratoInput.attr(
														'placeholder',
														'Número do Contrato');
												objetoInput.attr('placeholder',
														'Objeto do Contrato');
											}
											populateSubTipo(
													'#inputSubtipoMinuta',
													$(this));
										});

								tpModelo.change(function() {
									populateSubTipo('#inputSubtipoModelo',
											$(this));
								});

								$("input[name='tipoBusca']").change(function() {
									toggleTipoBusca();
								});

								$("#buscaSubmit").click(function() {
									submitBusca();
								});

								$("#finalizarSubmit")
										.click(
												function() {
													submitForm('/SIGATESTES/administracao/Processo/apresentaListar.action');
												});

								$("#modeloSubmit").click(function() {
									submitCriarModelo();
								}).toggle();

								$("#btnReset")
										.click(
												function() {
													$("input:text, select")
															.each(
																	function() {
																		$(this)
																				.val(
																						$(
																								this)
																								.prop(
																										"defaultValue"));
																	});
												});
							});
		</script>

