            <script type="text/javascript">
            $(document).ready(function () {

            	$('#lnkCriar').click(function(e) {
                	$("#frmUsuario").attr("action", "/SIGATESTES/administracao/Usuario/apresentaCriar.action");
                	$("#frmUsuario").submit();
                });

            	$('#lnkConsultar').click(function(e) {
            		if(!$("#usuario").val()
	            			&& !$("#login").val()
	            			&& !$("#unidade").val()
	            			&& !$("#departamento").val()
	               			&& !$("#almoxarifado").val()
	               			&& !$("#status").val()
	               			&& !$("#perfil").val()
	               		) {
	               			var texto = "Informe um parâmetro de busca.";
			            	error(texto);
			            	return false;
	               		}
            		$('#frmUsuario').submit();
                });

				$('#lnkLimpar').click(function () {
					$('#frmUsuario').trigger("reset");
                });

            });

            function error(texto){
            	$("#mensagens #texto").html(texto);
            	$("#mensagens").modal('show');
            }

        </script>
