            <script type="text/javascript">
  $(document).ready(function () {
      $('#penalidadeFornecedorBuscar').submit(function () {
          if (
            !$("#idSancaoBusca").val()
            && !$("#idTipoSancaoBusca").val()
          )
          {
              var texto = "Informe um parâmetro de busca.";
              error(texto);
              return false;
          }
      });

      $('#sancaoFornecedoLimpar').click(function () {
          $('#penalidadeFornecedorBuscar').trigger("reset");
      });

      //removendo a opção 'Não Definido' da combo de ações.
      $("#acaoSancao option[value='0']").remove();
  });

  function error(texto) {
      $("#mensagens #texto").html(texto);
      $("#mensagens").modal('show');
  }

  function loadAjax()
  {
    var h = "<img id='ajax' src='static/images/ajax-loader.gif'>";
    $("#ajaxLoader").html(h);
  }

  function unloadAjax()
  {
    $("#ajaxLoader").html("");
  }

  function setForm(acao)
  {
    if(acao=="criar")
    {
      $("#tituloPopup").text("Criar");
      $("#idSancao").hide();
      $("#idSancao").val("");
      $("#nomeSancao").val("");
      $("#nomeSancao").show();
    }
    else if(acao=="editar")
    {
      $("#tituloPopup").text("Editar");
      $("#idSancao").val("");
      $("#idSancao").show();
      $("#nomeSancao").hide();
      $("#nomeSancao").val("");

    }
    $("#tipoSancao").val("");
    $("#referenciaLei").val("");
    $("#descricao").val("");
    $("#acaoSancao").val("");
    $("#restricoes_FormN").removeAttr('checked');
    $("#restricoes_FormS").removeAttr('checked');
  }

  function validateForm()
  {
    //Setando os campos obrigatórios
    var nomeSancao = $("#nomeSancao").val();
    var idSancao = $("#idSancao").val();
    var tipoSancao = $("#tipoSancao").val();
    var referenciaLei = $("#referenciaLei").val();
    var descricao = $("#descricao").val();
    var acaoSancao = $("#acaoSancao").val();
    var modoCriar = "Criar";
    var modoEditar = "Editar";

    //Checando se os campos obrigatórios estão preenchidos
    if(
      (
        $("#tituloPopup").text() == modoCriar
        && nomeSancao
        && tipoSancao
        && tipoSancao != "-1"
        && referenciaLei
        && descricao
        && acaoSancao
        && acaoSancao != "-1"
      )
      ||
      (
        $("#tituloPopup").text() == modoEditar
        && idSancao
        && idSancao != "-1"
        && tipoSancao
        && tipoSancao != "-1"
        && referenciaLei
        && descricao
        && acaoSancao
        && acaoSancao != "-1"
      )
    )
    { //Caso estejam, submeter o formulário
      $("#frmPenalidadeFornecedor").submit();
    }
    else
    { //caso não estejam, exibir as mensagens de erro.
      var texto = "";
      if($("#tituloPopup").text() == modoCriar && !nomeSancao)
      {
        texto += "<span>Informe a(o) Nome.</span><br/>";
      }

      if($("#tituloPopup").text() == modoEditar && (!idSancao || idSancao == "-1"))
      {
        texto += "<span>Informe a(o) Nome.</span><br/>";
      }

      if(!tipoSancao || tipoSancao == "-1")
      {
        texto += "<span>Informe a(o) Tipo.</span><br/>";
      }

      if(!referenciaLei)
      {
        texto += "<span>Informe a(o) Referência da Lei.</span><br/>";
      }

      if(!descricao)
      {
        texto += "<span>Informe a(o) Descrição.</span><br/>";
      }
      if(!acaoSancao || acaoSancao == "-1")
      {
        texto += "<span>Informe a(o) Ação.</span><br/>";
      }

      error(texto);
    }
  }

  function carregaPenalidadeFornecedor()
  {
    loadAjax();
    var idSancao = $("#idSancao").val();
    $.getJSON("/SIGATESTES/administracao/PenalidadeFornecedor/carregaPenalidadeFornecedor.action", {
        idSancao: $("#idSancao").val()
    }, function (json) {
        ///////////////////////////////////////////////////
        unloadAjax();
        if(json.formbean!=null) {
          $("#tipoSancao").val(""+json.formbean.tipoSancao.id);
          alert(""+json.formbean.impedido);
          var imp = ""+json.formbean.impedido;
          if(imp == "S")
          {
            //$("#restricoes_FormN").removeAttr('checked');
            //$("#restricoes_FormS").attr('checked','checked');
            document.frmPenalidadeFornecedor.impedido[0].checked = true;
          }
          else if(imp == "N")
          {
            //$("#restricoes_FormS").removeAttr('checked');
            //$("#restricoes_FormN").attr('checked','checked');
            document.frmPenalidadeFornecedor.impedido[1].checked = true;
          }
          else
          {
            $("#restricoes_FormN").removeAttr('checked');
            $("#restricoes_FormS").removeAttr('checked');
          }
          $("#referenciaLei").val(""+json.formbean.fundamentoLegal);
          $("#descricao").val(""+json.formbean.descricao);
          $("#acaoSancao").val(""+json.formbean.acao.id);
          alert(""+json.formbean.acao.id);
        }
    });
  }
</script>

