/* RESPONSIVE CSS
-------------------------------------------------- */

@media (min-width: 768px) {
    /* Remove the edge padding needed for mobile */
    .basicContent {
        padding-left: 0;
        padding-right: 0;
        margin-top: 140px;
    }
    /* logo positioning foo */
    .logosiga {
        position: fixed;
        float: left;
        top: 10px;
    }
    .gov {
        position: absolute;
        right: 30px;
        top: 15px;
        width: 257px;
    }
    /* Navbar positioning foo */
    .navbar-wrapper {
        background: url(../images/fundo.jpg) repeat-x;
        padding-top: 20px;
        position: fixed;
        height: 150px;
    }
    .navbar-wrapper .container {
        padding-left: 15px;
        padding-right: 15px;
        margin: 0;
    }
    .navbar-wrapper .navbar {
        padding-left: 0;
        padding-right: 0;
    }
    /* The navbar becomes detached from the top, so we round the corners */
    .navbar-wrapper .navbar {
        position: relative;
        top: 74px;
    }
    /* Bump up size of carousel content */
    .carousel-caption p {
        margin-bottom: 20px;
        font-size: 21px;
        line-height: 1.4;
    }
    .featurette-heading {
        font-size: 50px;
    }
}
@media (min-width: 992px) {
    .featurette-heading {
        margin-top: 120px;
    }
}
