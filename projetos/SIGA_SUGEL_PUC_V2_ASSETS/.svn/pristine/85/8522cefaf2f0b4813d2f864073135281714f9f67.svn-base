/* GLOBAL STYLES
-------------------------------------------------- */

/* Padding below the footer and lighter body text */

body {
    padding-bottom: 40px;
    color: #5a5a5a;
}
/* CUSTOMIZE THE NAVBAR
-------------------------------------------------- */

/* Special class on .container surrounding .navbar, used for positioning it into place. */

.navbar-wrapper {
    background: none repeat scroll 0 0 #003581;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    z-index: 20;
}
/* Flip around the padding for proper display in narrow viewports */

.navbar-wrapper .container {
    padding-left: 0;
    padding-right: 0;
}
.navbar-wrapper .navbar {
    padding-left: 15px;
    padding-right: 15px;
}
/* CUSTOMIZE THE CAROUSEL
-------------------------------------------------- */

/* Carousel base class */

.carousel {
    height: 500px;
    margin-bottom: 60px;
}
/* Since positioning the image, we need to help out the caption */

.carousel-caption {
    z-index: 10;
}
/* Declare heights because of positioning of img element */

.carousel .item {
    height: 500px;
    background-color: #777;
}
.carousel-inner > .item > img {
    position: absolute;
    top: 0;
    left: 0;
    min-width: 100%;
    height: 500px;
}
/* User BOX CONTENT
-------------------------------------------------- */

.user-box {
    height: 65px;
    position: absolute;
    right: 300px;
}
.user-box-inner {
    display: inline-block;
    text-align: left;
}
.user-box .user-avatar {
    display: block;
    float: left;
    margin-right: 10px;
    max-width: 40px;
    position: relative;
    top: 2px;
}
.user-box .user-info {
    display: inline-block;
    padding: 10px 20px 0 0;
    color: #fff;
}
.user-box .user-info ul {
    line-height: 15px;
    margin: 0;
    padding-left: 0;
}
.user-box .user-info li {
    display: inline;
    font-size: 11px;
}
.user-box .user-info li a {
    color: #3699D2;
}
.img-avatar {
    background: none repeat scroll 0 0 #5276aa;
    border: 1px solid #DDDDDD;
    border-radius: 4px 4px 4px 4px;
    padding: 2px;
}
.actions {
    position: fixed;
    right: 85px;
    top: 102px;
    z-index: 100;
}
/* MARKETING CONTENT
-------------------------------------------------- */

/* Pad the edges of the mobile views a bit */

.basicContent {
    padding-left: 15px;
    padding-right: 15px;
    margin-top: 120px;
}
/* Center align the text within the three columns below the carousel */

.basicContent .col-lg-4 {
    text-align: center;
    margin-bottom: 20px;
}
.basicContent h2 {
    font-weight: normal;
}
.basicContent .col-lg-4 p {
    margin-left: 10px;
    margin-right: 10px;
}
